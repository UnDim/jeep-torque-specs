<!DOCTYPE html>
<html>
<head>
  <%- include ("../partials/header.ejs") %>
</head>

<body>

<%- include ("../partials/nav.ejs") %>

<div class="container">
<h2>Database Results</h2>

<table>
  <tr>
    <th>ID</th>
    <th>Category</th> 
    <th>Desciption</th>
    <th>N-M</th>
    <th>FT-LBS</th>
    <th>IN-LBS</th>
    <th>Comments</th>
  </tr>
  <tr>
    <td>Centro comercial Moctezuma</td>
    <td>Francisco Chang</td> 
    <td>Mexico</td>
  </tr>
</table>

<ul>
    <% results.forEach(function(r) { %>
        <li><%= r.id %> - <%= r.category %> - <%= r.description %> - <%= r.n_m %> - <%= r.ft_lbs %> - <%= r.in_lbs %> - <%= r.comments %></li>
    <% }); %>
</ul>

</div>

</body>
</html>
